        <messages/>
        <name>Restrict state choice list</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    var userRoles = g_user.roles;
	var currentChoice = g_form.getValue('state');
	//g_form.addInfoMessage('Current: ' + currentChoice);

    var choices = [];

    if (userRoles.indexOf('sn_contingent_mgmt.contingent_hiring_manager') !== -1) {
        // Admin role-specific choices
        choices = [{
                value: 1,
                label: 'Requested'
            },
            {
                value: 2,
                label: 'Submit'
            }
        ];
    } else if(userRoles.indexOf('sn_contingent_mgmt.contingent_owner') !== -1){
        // Other role-specific choices
        choices = [{
                value: 2,
                label: 'Submit'
            },
			{
                value: 3,
                label: 'Onboarded'
            },
            {
                value: 7,
                label: 'Closed Complete'
            }
        ];
    }else{
		choices = [{
                value: 1,
                label: 'Requested'
            },
            {
                value: 2,
                label: 'Submit'
            },
			{
                value: 3,
                label: 'Onboarded'
            },
            {
                value: 7,
                label: 'Closed Complete'
            }
        ];
	}

    // Set the choices for the choice list field
    g_form.clearOptions('state'); // Replace with your actual choice list field name
    choices.forEach(function(choice) {
        g_form.addOption('state', choice.value, choice.label);
    });
	g_form.setValue('state',currentChoice);
}]]></script>
