        <messages/>
        <name>Restrict state choice list field</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    var userRoles = g_user.roles;

    // Get the choice list field element
    var choiceListField = g_form.getControl('state'); // Replace with your actual choice list field name

    // Define the choices based on user role
    var choices = [];

    if (userRoles.indexOf('sn_contingent_mgmt.contingent_hiring_manager') !== -1) {
        // Admin role-specific choices
        choices = [1, 2];
    } else {
        // Other role-specific choices
        choices = [3, 7];
    }

    // Set the choices for the choice list field
    g_form.clearOptions('state'); // Replace with your actual choice list field name
    g_form.addOptions('state', choices, choices);
}]]></script>
